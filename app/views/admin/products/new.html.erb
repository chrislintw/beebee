<h1>新增產品</h1>
<div class="ibox">
  
  <div class="ibox-content">
      <%= simple_form_for @admin_product do |f| %>
      <div class="form-inputs">
        <%= f.input :image %>
        <%= f.input :name %>
        <%= f.input :price %>
        <%= f.input :discount %>
        
      </div>  
      <div class="form-actions">
        <%= f.submit '確定', class: 'btn btn-success' %>
      </div>
    <% end %>
  </div>
</div>


